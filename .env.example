# .env.example - Copy this to .env and fill in your own values

# REQUIRED: OpenAI API Key for AI Agent functionality
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Other AI providers
# ANTHROPIC_API_KEY=your_anthropic_key
# COHERE_API_KEY=your_cohere_key

# AWS Configuration (for Bedrock integration - optional)
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_REGION=us-east-1

# Agent Configuration
AGENT_NAME=RestaurantAssistant
AGENT_VERSION=1.0.0
MAX_CONTEXT_LENGTH=4000
TEMPERATURE=0.7

# Flask Configuration (optional - defaults are fine for most cases)
# FLASK_ENV=production
# FLASK_DEBUG=False
# SECRET_KEY=your_secret_key_here
